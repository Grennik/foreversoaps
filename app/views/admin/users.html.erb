<div class="admin-panel">
  <div class="admin-top">
      <span class="listing-head">Users</span>
      <span class="add-listing">
      </span>
    </div>
    <table class='listing-table table'>
      <thead class="bold">
        <td>ID</td>
        <td>Email</td>
      </thead>
      <tbody>

        <% @users.each do |c| %>
        <tr>
          <td><%= c.id %></td>
          <td><%= c.email %></td>
          <td><%= link_to "test email", :controller => "admin", :action => "new_user", :id => c %>
        </tr>
        <% end %>

      </tbody>
    </table>
  </div>
